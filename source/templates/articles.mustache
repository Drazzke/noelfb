{{#articles}}
				<div class="work article work-{{id}}">
					<div class="postcard" data-bg="img/{{id}}/postcard.png"></div>
					<h1>{{title}}</h1>
					<div class="info">{{{body}}}</div>
					{{#disqus}}
					<div class="disqus" data-disqus="{{disqus}}"></div>
					{{/disqus}}
				</div>
{{/articles}}